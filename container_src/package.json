{
  "name": "@defikitteam/claude-acp-client",
  "version": "0.1.0",
  "description": "Lightweight ACP client for Claude Code with remote worker communication",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "claude-acp-client": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "acp": "node dist/index.js",
    "http-bridge": "node dist/index.js --http-bridge",
    "test": "vitest",
    "check:lines": "node scripts/check-line-limits.mjs"
  },
  "keywords": [
    "claude",
    "acp",
    "agent",
    "anthropic",
    "typescript",
    "sdk",
    "code",
    "lightweight",
    "client"
  ],
  "dependencies": {
    "@anthropic-ai/claude-code": "1.0.113",
    "@octokit/rest": "^22.0.0",
    "@modelcontextprotocol/sdk": "1.18.0",
    "@zed-industries/agent-client-protocol": "0.3.1",
    "diff": "8.0.2",
    "minimist": "1.2.8",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/minimist": "1.2.5",
    "@types/node": "24.4.0",
    "@types/uuid": "10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
