# ============================================================================
# Container Environment Variables
# ============================================================================
# Copy this file to .env and fill in your actual values
# These variables are used by the Node.js container running Claude Code SDK
# ============================================================================

# ============================================================================
# LLM Providers (EXECUTION)
# ============================================================================

# Anthropic API key - Core dependency for Claude Code SDK
ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_key_here

# OpenHands API key for OpenHands adapter
OPENHANDS_API_KEY=your_openhands_key_here

# OpenRouter API key for OpenRouter adapter (if used by container)
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_key_here

# GitHub personal access token for git operations
GITHUB_TOKEN=ghp_your_github_token_here

# Target repository for automation testing
GITHUB_AUTOMATION_REPOSITORY=your-org/your-repo
GITHUB_AUTOMATION_DEFAULT_BRANCH=main
